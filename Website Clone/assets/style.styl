@font-face
    font-family "MarkOT-Regular"
    src url("font/MarkOT-Regular.woff") format("woff"),url("fonts/MarkOT-Regular.ttf") format("truetype")
    font-weight normal
    font-style normal
@font-face
    font-family "MarkOT-Medium"
    src url("fonts/MarkOT-Medium.woff") format("woff"),url("fonts/MarkOT-Medium.ttf") format("truetype")
    font-weight normal
    font-style normal
@font-face
    font-family "MarkOT-Bold"
    src url("fonts/MarkOT-Bold.woff") format("woff"),url("fonts/MarkOT-Bold.ttf") format("truetype")
    font-weight normal
    font-style normal
body
    margin 0
    padding 0
    box-sizing border-box
.nav
    height auto
    width 100%
    background-color rgb(34, 33, 33)
    overflow hidden
    display flex
    justify-content space-around
    gap 20%
    &list
        list-style-type none
        margin 0
        padding 0
        overflow hidden
        &-container
            margin auto
.logo
    float left
    width auto
    margin 2% 0 1% 10%
    padding 5px
.searchlogo
    float left
    margin 2% 0 1% -2%
    padding 5px
li
  float left
  a
    display block
    color white
    text-align center
    padding 14px 10px
    font-family "Lucida Sans", "Lucida Sans Regular", "Lucida Grande","Lucida Sans Unicode", Geneva, Verdana, sans-serif
    font-size 12px
    font-weight 500
    text-decoration none
    
        
    
